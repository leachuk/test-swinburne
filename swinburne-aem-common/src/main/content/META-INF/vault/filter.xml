<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">
    <!--                        W A R N I N G                     -->
    <!--        This change can make the AEM content removed      -->
    <!--    http://jackrabbit.apache.org/filevault/filter.html    -->
    <!--                        W A R N I N G                     -->

    <filter root="/apps/cq/analytics" />

    <filter root="/apps/sling" />

    <filter root="/apps/swinburne">
        <!-- The type of the first pattern defines the default behavior, so that the filter is more natural to write.
        If the first pattern is include, then the default is exclude and vice versa.  -->
        <exclude pattern="/apps/swinburne/config(.*)?"/>
    </filter>

    <filter root="/etc/blueprints/swinburne" mode="update"/>
    <filter root="/etc/designs/swinburne" mode="update"/>

    <filter root="/etc/clientlibs/swinburne/core"/>

    <filter root="/etc/clientlibs/swinburne/v1">
        <!-- do not allow deploy into css js and fonts folders -->
        <include pattern="/etc/clientlibs/swinburne/v1/.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/v1/css.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/v1/js.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/v1/fonts.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/v1/images.*"/>
    </filter>

    <filter root="/etc/clientlibs/swinburne/v2">
        <!-- do not allow deploy into css js and fonts folders -->
        <include pattern="/etc/clientlibs/swinburne/v2/.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/v2/css.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/v2/js.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/v2/fonts.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/v2/images.*"/>
    </filter>

    <filter root="/etc/clientlibs/swinburne/styleguide">
        <!-- do not allow deploy into css js and fonts folders -->
        <include pattern="/etc/clientlibs/swinburne/styleguide/.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/styleguide/css.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/styleguide/js.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/styleguide/fonts.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/styleguide/images.*"/>
    </filter>

    <!--<filter root="/apps/settings/wcm/design/swinburne"/>-->

    <!--<filter root="/etc/clientlibs/swinburne">-->
        <!--&lt;!&ndash; do not allow deploy into css js and fonts folders &ndash;&gt;-->
        <!--<include pattern="/etc/clientlibs/swinburne/.*"/>-->
        <!--<exclude pattern="/etc/clientlibs/swinburne/css.*"/>-->
        <!--<exclude pattern="/etc/clientlibs/swinburne/js.*"/>-->
        <!--<exclude pattern="/etc/clientlibs/swinburne/fonts.*"/>-->
        <!--<exclude pattern="/etc/clientlibs/swinburne/images.*"/>-->
    <!--</filter>-->

    <!-- required for templates -->
    <!--<filter root="/conf/swinburne/install"></filter>-->


</workspaceFilter>
