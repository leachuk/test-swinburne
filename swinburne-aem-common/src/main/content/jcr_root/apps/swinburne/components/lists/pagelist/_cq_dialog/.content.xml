<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Page List"
          sling:resourceType="cq/gui/components/authoring/dialog">
    <content
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/container">
        <layout
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/layouts/tabs"
                type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <listTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="List"
                    sling:resourceType="granite/ui/components/coral/foundation/include"
                    path="/apps/aemdesign/components/lists/list/cq:dialog/content/items/listTab"
                    resourceType="granite/ui/components/foundation/section"/>
            <listConfigTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="List Config"
                    sling:resourceType="granite/ui/components/coral/foundation/include"
                    path="/apps/aemdesign/components/lists/list/cq:dialog/content/items/listConfigTab"
                    resourceType="granite/ui/components/foundation/section"/>
            <layoutTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Layout"
                    sling:resourceType="granite/ui/components/foundation/section">
                <layout
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                        margin="{Boolean}false"/>
                <items jcr:primaryType="nt:unstructured">
                    <column
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <variant
                                    jcr:primaryType="nt:unstructured"
                                    jcr:title="Layout"
                                    sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
                                <items jcr:primaryType="nt:unstructured">
                                    <detailsBadge
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/foundation/form/select"
                                            fieldDescription="Which badge to select from the Details component on the page"
                                            fieldLabel="Use Badge"
                                            name="./detailsBadge"
                                            value="">
                                        <items jcr:primaryType="nt:unstructured">
                                            <default
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Default"
                                                    value="default"/>
                                            <icon
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Icon"
                                                    value="icon"/>
                                            <image
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Image"
                                                    value="image"/>
                                            <card
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Image"
                                                    value="card"/>
                                            <cardIcon
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Icon"
                                                    value="cardIcon"/>
                                            <cardIconTitleCategoryDescriptionAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Icon, Title, Category, Description and Action"
                                                    value="cardIconTitleCategoryDescriptionAction"/>
                                            <cardImageTagTitleAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Image, Tag, Title and Action"
                                                    value="cardImageTagTitleAction"/>
                                            <cardImageTitleCategoryDescriptionAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Image, Title, Category, Description and Action"
                                                    value="cardImageTitleCategoryDescriptionAction"/>
                                            <cardIconTitleCategoryDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Clickable Card with Icon"
                                                    value="cardIconTitleCategoryDescription"/>
                                            <cardImageTitleCategoryDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Clickable Card with Image "
                                                    value="cardImageTitleCategoryDescription"/>
                                            <cardHorizontalIconTitleCategoryDescriptionAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Horizontal Card with Icon, Category, Description and Action"
                                                    value="cardHorizontalIconTitleCategoryDescriptionAction"/>
                                            <cardTitleDescriptionAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Title, Description and Action"
                                                    value="cardTitleDescriptionAction"/>
                                            <cardImageTitleSubtitleDescriptionAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Image, Title, Sub Title, Description and Action"
                                                    value="cardImageTitleSubtitleDescriptionAction"/>
                                            <cardImageTitleDescriptionAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Image, Title, Description and Action"
                                                    value="cardImageTitleDescriptionAction"/>
                                            <cardImageTitleAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Image, Title and Action"
                                                    value="cardImageTitleAction"/>
                                            <cardImageTitleDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Image, Title and Description"
                                                    value="cardImageTitleDescription"/>
                                            <cardIconDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Icon and Description"
                                                    value="cardIconDescription"/>
                                            <cardIconTitleDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Icon, Title and Description"
                                                    value="cardIconTitleDescription"/>
                                            <cardIconTitleDescriptionAction
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Card with Icon, Title, Description and Action"
                                                    value="cardIconTitleDescriptionAction"/>
                                            <cardActionImageTitle
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Clickable Card with Image and Title"
                                                    value="cardActionImageTitle"/>
                                            <cardActionIconTitleCategoryDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Clickable Card with Icon, Title, Category and Description"
                                                    value="cardActionIconTitleCategoryDescription"/>
                                            <cardActionImageTitleDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Clickable Card with Image, Title and Description"
                                                    value="cardActionImageTitleDescription"/>
                                            <cardActionIconDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Clickable Card with Icon and Description"
                                                    value="cardActionIconDescription"/>
                                            <cardActionImageTitleCategoryDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Clickable Card with Image, Title, Category and Description "
                                                    value="cardActionImageTitleCategoryDescription"/>
                                            <cardActionIconTitleDescription
                                                    jcr:primaryType="nt:unstructured"
                                                    text="Clickable Card with Icon, Title and Description"
                                                    value="cardActionIconTitleDescription"/>
                                        </items>
                                    </detailsBadge>
                                </items>
                            </variant>
                        </items>
                    </column>
                </items>
            </layoutTab>
            <badgeConfigTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Badge Config"
                    sling:resourceType="granite/ui/components/coral/foundation/include"
                    path="/apps/aemdesign/global/dialog/touch/badge/content/items/badgeConfigTab"
                    resourceType="granite/ui/components/foundation/section"/>
            <badgeThumbnailTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Badge Thumbnail"
                    sling:resourceType="granite/ui/components/coral/foundation/include"
                    path="/apps/aemdesign/global/dialog/touch/asset/content/items/badgeThumbnailTab"
                    resourceType="granite/ui/components/foundation/section"/>
            <styleTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Style"
                    sling:resourceType="granite/ui/components/coral/foundation/include"
                    path="/apps/aemdesign/global/dialog/touch/style/content/items/styleTab"
                    resourceType="granite/ui/components/foundation/section"/>
            <accessibilityTab
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Accessibility"
                    sling:resourceType="granite/ui/components/coral/foundation/include"
                    path="/apps/aemdesign/global/dialog/touch/accessibility/content/items/accessibilityTab"
                    resourceType="granite/ui/components/foundation/section"/>
        </items>
    </content>
</jcr:root>
