<?xml version="1.0" encoding="UTF-8"?>
<workspaceFilter version="1.0">

    <filter root="/content/dam/swinburne"/>
    <filter root="/content/swinburne"/>

    <filter root="/content/api/swinburne" mode="update"/>
    <filter root="/content/experience-fragments/swinburne" mode="update"/>

    <filter root="/etc/blueprints/swinburne" mode="update"/>
    <filter root="/etc/designs/swinburne" mode="update"/>

    <filter root="/conf/swinburne" mode="update"/>

    <filter root="/etc/clientlibs/swinburne">
        <!-- do not allow deploy into css js and fonts folders -->
        <include pattern="/etc/clientlibs/swinburne/.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/css.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/js.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/fonts.*"/>
        <exclude pattern="/etc/clientlibs/swinburne/images.*"/>
    </filter>

    <filter root="/content/dam/formsanddocuments-themes/swinburne"/>
    <filter root="/etc/clientlibs/swinburne/forms"/>

</workspaceFilter>
