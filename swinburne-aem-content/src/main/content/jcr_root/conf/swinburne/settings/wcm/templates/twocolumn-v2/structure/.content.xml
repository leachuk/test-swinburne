<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[/etc/mobile/groups/responsive]"
        cq:template="/conf/swinburne/settings/wcm/templates/twocolumn-v2"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="swinburne/components/template/column/two/v2">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/header"
            componentId="header_SYFWWFLMP"
            editable="{Boolean}true">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"
                editable="{Boolean}true"/>
        </header>
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/aside"
            componentId="aside_5MP5ASEDY"
            editable="{Boolean}true">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"
                editable="{Boolean}true"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article"
            componentId="article_ZLSGLU3CN"
            editable="{Boolean}true">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"
                editable="{Boolean}true"/>
        </article>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/footer"
            componentId="footer_O5Y9NZIZ2"
            editable="{Boolean}true">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"
                editable="{Boolean}true"/>
        </footer>
        <cq:responsive jcr:primaryType="nt:unstructured">
            <breakpoints jcr:primaryType="nt:unstructured">
                <xs
                    jcr:primaryType="nt:unstructured"
                    title="XS - Phone"
                    width="{Long}320"/>
                <sm
                    jcr:primaryType="nt:unstructured"
                    title="SM - Phone"
                    width="{Long}640"/>
                <md
                    jcr:primaryType="nt:unstructured"
                    title="MD - Tablet"
                    width="{Long}1024"/>
                <lg
                    jcr:primaryType="nt:unstructured"
                    title="LG - Laptop"
                    width="{Long}1024"/>
                <xlg
                    jcr:primaryType="nt:unstructured"
                    title="XLG - Desktop"
                    width="{Long}1024"/>
                <xxlg
                    jcr:primaryType="nt:unstructured"
                    title="XXLG - 4K"
                    width="{Long}1024"/>
            </breakpoints>
        </cq:responsive>
    </jcr:content>
</jcr:root>
