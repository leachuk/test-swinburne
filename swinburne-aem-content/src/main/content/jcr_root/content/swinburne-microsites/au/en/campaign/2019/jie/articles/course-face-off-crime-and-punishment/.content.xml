                            componentId="text_W6RDSSPZB"
                            componentModifiers="[swinburne:component-style-modifier/padding/pb-15]"
                            text="&lt;h1>Course Face-off: &lt;span class=&quot;text-c7&quot;>Crime and Punishment&lt;/span>&lt;/h1>&#xa;&lt;p style=&quot;text-align: center;&quot;>&lt;b>Welcome to our Course Face-off series where two academics back their course in a winner-takes-all battle for supremacy.&lt;/b>&lt;/p>&#xa;"
                            textIsRich="true"/>
                        <image
                            cq:lastRolledout="{Date}2019-01-24T04:40:36.930Z"
