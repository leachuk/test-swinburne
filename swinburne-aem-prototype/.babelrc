{
  "comments": true,
  "compact": "auto",
  "retainLines": true,

  "presets": [
    ["@babel/preset-env", {
      "modules": "umd",
      "useBuiltIns": "usage"
    }],
    "@babel/preset-typescript"
  ],

  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
