<!-- // icons \\ -->
<div class="container my-large">
  <div class="row">
      <h1>{{ icons_title }}</h1>
  </div>
  <h2>General</h2>
  <div class="row icon-list">
    {{# icons }}
        <div class="col-6 col-sm-4 col-md-3 col-lg-2 icon">
              <i class="fa fa-{{ value }}"></i>
              <div class="sg-label">{{ name }}</div>
        </div>
    {{/ icons }}
  </div>
  <h2>Socials</h2>
  <div class="row icon-list">
    {{# social_icons }}
      <div class="col-6 col-sm-4 col-md-3 col-lg-2 icon">
        <i class="fa fa-{{ value }}"></i>
        <div class="sg-label">{{ name }}</div>
      </div>
    {{/ social_icons }}
  </div>
</div>
