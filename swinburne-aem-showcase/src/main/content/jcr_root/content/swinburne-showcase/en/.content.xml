<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[swinburne:content-type/page]"
        cq:template="/conf/swinburne/settings/wcm/templates/twocolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:language="en_au"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="English"
        sling:resourceType="swinburne/components/template/base"
        hideInNav="true"
        redirectTarget="/content/swinburne-showcase/en/component">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/header"
            cancelInheritParent="true">
            <bgimage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/image"
                imageRotate="0"/>
            <logo
                jcr:primaryType="nt:unstructured"
                sling:resourceType="foundation/components/image"
                imageRotate="0"/>
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <text
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/content/text"
                        text="&lt;p>Header content to be replaced by Experience Fragment&lt;/p>&#xa;"
                        textIsRich="true"/>
            </par>
        </header>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/footer"
            cancelInheritParent="true">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <text
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/content/text"
                        text="&lt;p>Footer content to be replaced by Experience Fragment&lt;/p>&#xa;"
                        textIsRich="true"/>
            </par>
        </footer>
    </jcr:content>
    <admin/>
    <pages/>
    <templates/>
    <component/>
    <content/>
</jcr:root>
