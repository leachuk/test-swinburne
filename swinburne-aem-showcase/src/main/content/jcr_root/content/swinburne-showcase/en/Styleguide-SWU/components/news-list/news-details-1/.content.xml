                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"
                        fileReference="/content/dam/swinburne-showcase/en/components/content/download/network.jpg"/>
                    <bgimage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
