<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/swinburne/settings/wcm/templates/styleguide"
        jcr:primaryType="cq:PageContent"
        jcr:title="Template"
        sling:resourceType="swinburne/components/template/styleguide">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/aside"
            componentId="aside_I5D3YECFG">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article"
            componentId="article_VRHELJPP8">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page_details
                        jcr:mixinTypes="[cq:Taggable]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/details/page-details"
                        editable="{Boolean}false"
                        componentId="page_details"
                        componentTheme="[swinburne-showcase:component-style-theme/styleguide/page-details]"
                        showBreadcrumb="false"
                        showPageDate="false"
                        showParsys="false"
                        showToolbar="false"
                        textIsRich="true" />
                <contentblock
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="contentblock"
                        componentModifiers="[swinburne-showcase:component-style-modifier/padding/p-15,swinburne-showcase:component-style-modifier/padding/p-md-30]">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <contentblock_1210338601
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/layout/contentblock"
                                componentId="contentblock1">
                            <par
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/layout/container">
                                <text
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="swinburne/components/content/text"
                                        componentId="description-1"
                                        componentModifiers="[swinburne-showcase:component-style-modifier/margin/mb-10]"
                                        text="&lt;p>Use the &lt;b>Swinburne Global Single Column &lt;/b>template for all pages. The template consists of the header and footer, with an content section between them.&lt;/p>&#xd;&#xa;"
                                        textIsRich="true"/>
                                <image
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="swinburne/components/media/image"
                                        componentId="template-image"
                                        componentModifiers="[swinburne-showcase:component-style-modifier/image/responsive,swinburne-showcase:component-style-modifier/margin/mb-15]"
                                        fileReference="/content/dam/swinburne-showcase/en/styleguide/Swinburne Global One Column.png"/>
                                <text_1505806541
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="swinburne/components/content/text"
                                        componentId="description-2"
                                        text="&lt;p>If you need to insert multiple columns within the content area, use the &lt;b>Column Control &lt;/b>component.&lt;/p>&#xd;&#xa;"
                                        textIsRich="true"/>
                            </par>
                            <image
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="swinburne/components/media/image"
                                    componentId="image_7ZLU3OH5A"/>
                            <bgimage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/media/image"/>
                            <bgvideo
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="aemdesign/components/media/image"/>
                        </contentblock_1210338601>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
