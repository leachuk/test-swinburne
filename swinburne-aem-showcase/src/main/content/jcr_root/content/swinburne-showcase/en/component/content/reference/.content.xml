<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[swinburne:content-type/page]"
        cq:template="/conf/swinburne/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Content Reference"
        sling:resourceType="swinburne/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/details/page-details"
                    componentId="page-details_QA0F4WWRR"/>
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblock"
                    componentId="contentblock1"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="Variant: Default"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/container">
                        <reference
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/reference"
                            componentId="reference1"
                            path="/content/swinburne-showcase/en/component/content/reference/page1/jcr:content/article"/>
                    </par>
                </contentblock1>
                <contentblock2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblock"
                    componentId="contentblock2"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="Variant: Render"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/container">
                        <reference
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/reference"
                            componentId="reference2"
                            path="/content/swinburne-showcase/en/component/content/reference/page1/jcr:content/article"
                            variant="render"/>
                    </par>
                </contentblock2>
            </par>
        </article>
    </jcr:content>
    <page2/>
    <page1/>
</jcr:root>
