<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:tags="[swinburne:content-type/page]"
            cq:template="/conf/swinburne/settings/wcm/templates/twocolumn"
            jcr:primaryType="cq:PageContent"
            jcr:title="Contact Details"
            sling:resourceType="swinburne/components/template/base">
        <article
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/article">
            <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/container">
                <contentblockmenu1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblockmenu"
                        componentId="contentblockmenu1"/>

                <contentblock1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        title="Contact Details: Default without included components"
                        variant="advsection"
                        componentId="contentblock1"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                text=""
                                textIsRich="true"
                                variant="simple"/>
                        <contactdetails
                                jcr:mixinTypes="[cq:Taggable]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/details/contact-details"
                                componentId="contact-details1"
                                description="Custom Description"
                                descriptionFormat="swinburne:component-style-theme/details/contact-details/format/description/jobTitleAtEmployeeWithEmail"
                                email="max.barrass@isobar.com"
                                employee="Isobar"
                                familyName="Barrass"
                                givenName="Max"
                                honorificPrefix="swinburne:component-dialog/honorific/prefix/mr"
                                jobTitle="Adobe Practice lead"
                                showBreadcrumb="false"
                                showParsys="false"
                                showToolbar="false"
                                title="Page Title"
                                titleFormat="swinburne:component-style-theme/details/contact-details/format/title/author">
                        </contactdetails>
                    </par>
                </contentblock1>
                <contentblock2
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        title="Contact Details: Default Blank without included components"
                        variant="advsection"
                        componentId="contentblock1"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                text=""
                                textIsRich="true"
                                variant="simple"/>
                        <contactdetails
                                jcr:mixinTypes="[cq:Taggable]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/details/contact-details"
                                componentId="contact-details2"
                                showBreadcrumb="false"
                                showToolbar="false"
                                showParsys="false">
                        </contactdetails>
                    </par>
                </contentblock2>
                <contentblock3
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        title="Contact Details: Hidden Variant"
                        variant="advsection"
                        componentId="contentblock1"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                text=""
                                textIsRich="true"
                                variant="simple"/>
                        <contactdetails
                                jcr:mixinTypes="[cq:Taggable]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/details/contact-details"
                                componentId="contact-details3"
                                variant="hidden"
                                showBreadcrumb="false"
                                showToolbar="false"
                                showParsys="false">
                        </contactdetails>
                    </par>
                </contentblock3>
            </par>
        </article>
        <image
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/media/image"
                fileReference="/content/dam/swinburne-showcase/en/common/images/abstract.jpg"/>
    </jcr:content>
</jcr:root>
