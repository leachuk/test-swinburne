<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/swinburne/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Footer"
        sling:resourceType="swinburne/components/template/base">
        <article jcr:primaryType="nt:unstructured"
                 sling:resourceType="swinburne/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <contentblock1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblock"
                    componentId="contentblock1"
                    title="Variant: Default"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/container">
                        <footer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/footer"
                            cancelInheritParent="true"
                            componentId="footer1">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/layout/container">
                                <text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="swinburne/components/content/text"
                                    text="&lt;p>Footer Content&lt;/p>&#xa;"
                                    textIsRich="true"/>
                            </par>
                        </footer>
                    </par>
                </contentblock1>
                <contentblock2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblock"
                    componentId="contentblock2"
                    title="Variant: Default with Background"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/container">
                        <footer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/footer"
                            cancelInheritParent="true"
                            componentId="footer2">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/layout/container">
                                <text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="swinburne/components/content/text"
                                    text="&lt;p>Footer Content with Background&lt;/p>&#xa;"
                                    textIsRich="true"/>
                            </par>
                            <bgimage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="swinburne/components/media/image"
                                    fileReference="/content/dam/swinburne-showcase/en/common/aem-design.png"/>
                        </footer>
                    </par>
                </contentblock2>
                <contentblock3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblock"
                    componentId="contentblock3"
                    title="Variant: Default with Styles"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/container">
                        <footer
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/footer"
                            cancelInheritParent="true"
                            componentId="footer3"
                            componentModifiers="[swinburne:component-style-modifier/background/bg-c8,swinburne:component-style-modifier/color/standard/white]">
                            <par
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/layout/container">
                                <text
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="swinburne/components/content/text"
                                    text="&lt;p>Footer Content with Style&lt;/p>&#xa;"
                                    textIsRich="true"/>
                            </par>
                        </footer>
                    </par>
                </contentblock3>
            </par>
        </article>
    </jcr:content>
</jcr:root>
