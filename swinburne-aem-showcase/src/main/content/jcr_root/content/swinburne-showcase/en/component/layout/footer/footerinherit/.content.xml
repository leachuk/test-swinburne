<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/swinburne/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Footer Inherit"
        sling:resourceType="swinburne/components/template/base">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/header">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
        </header>
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <swinburne_global_footer
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="swinburne_global_footer">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <footer
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/layout/footer"
                                cancelInheritParent="false"
                                componentId="swinburne_global_footer_inherit">
                            <bgimage
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="swinburne/components/media/image"/>
                        </footer>
                    </par>
                </swinburne_global_footer>
            </par>
        </article>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/footer"
            cancelInheritParent="false"
            componentId="footer4">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
            <bgimage
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/media/image"/>
        </footer>
    </jcr:content>
</jcr:root>
