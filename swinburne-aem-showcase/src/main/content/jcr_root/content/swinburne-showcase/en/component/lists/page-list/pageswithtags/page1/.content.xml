<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[facebook:]"
        cq:template="/conf/swinburne-showcase/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Page1"
        sling:resourceType="swinburne/components/template/base"
        subtitle="Page Sub-Title 1">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page-details
                    cq:tags="[swinburne-showcase:content-type/page,swinburne-showcase:content-type/widgets]"
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/details/page-details"
                    cardStyle="[swinburne-showcase:component-style-modifier/background/bg-c8,swinburne-showcase:component-style-modifier/card/coloured]"
                    description="Page with Licensed Page Image, with non-Licensed Secondary Image and with Background non-Licensed Image"
                    showBreadcrumb="false"
                    showParsys="false"
                    showToolbar="false"
                    subCategory="[swinburne-showcase:content-type/asset]"
                    textIsRich="true"
                    title="Page1">
                    <secondaryImage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/media/image"
                        fileReference="/content/dam/swinburne-showcase/en/components/media/image/city2.jpg"/>
                    <bgimage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/media/image"
                        fileReference="/content/dam/swinburne-showcase/en/components/media/image/city3.jpg"/>
                    <thumbnail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/media/image"/>
                </page-details>
            </par>
        </article>
        <image
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/media/image"
            fileReference="/content/dam/swinburne-showcase/en/components/media/image/city1.jpg"/>
    </jcr:content>
</jcr:root>
