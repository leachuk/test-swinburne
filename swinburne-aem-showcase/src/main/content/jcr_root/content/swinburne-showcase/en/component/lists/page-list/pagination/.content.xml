<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/swinburne/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Pagination"
        sling:resourceType="swinburne/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/swinburne/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/details/page-details"/>
                <contentblock1
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        title="Search Widget"
                        variant="advsection"
                        componentId="contentblock1"
                        hideTitle="false"
                        hideTitleSeparator="true">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <text
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                text="Simple page list with pagination"
                                textIsRich="true"
                                variant="simple"/>
                        <pagelist
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/lists/pagelist"
                                componentId="pagelist1"
                                listFrom="children"
                                listType="list"
                                pageMax="1"
                                parentPage="/content/swinburne-showcase/en/component/lists/page-list/pagination"/>
                    </par>
                </contentblock1>
            </par>
        </article>
    </jcr:content>
    <page1/>
    <page2/>
    <page3/>
    <page4/>
    <page5/>
</jcr:root>
