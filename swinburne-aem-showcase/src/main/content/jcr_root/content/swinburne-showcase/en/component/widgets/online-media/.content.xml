<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[swinburne:content-type/page]"
        cq:template="/conf/swinburne/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Online Media"
        sling:resourceType="swinburne/components/template/base">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/details/page-details"/>
                <contentblockmenu
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblockmenu"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblock"
                    componentId="contentblock1"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="Online Media: Default Variant"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/container">
                        <onlinemedia
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/widgets/onlinemedia"
                            componentId="onlinemedia1"
                            mediaId="qxpEV3VY6T8"
                            provider="youtube"
                            providerUrl="https://www.youtube.com/embed/tL46xeIV5mc"
                            variant="default"/>
                    </par>
                    <bgimage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                    <bgvideo
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                </contentblock>
                <contentblock_copy
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblock"
                    componentId="contentblock2"
                    hideTitle="false"
                    hideTitleSeparator="true"
                    title="Online Media: Video  IFrame  Variant"
                    variant="advsection">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/container">
                        <onlinemedia
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/widgets/onlinemedia"
                            componentId="onlinemedia2"
                            mediaId="qxpEV3VY6T8"
                            positionX="400"
                            positionY="300"
                            provider="youtube"
                            providerUrl="https://player.vimeo.com/video/204029471?title=0&amp;byline=0&amp;portrait=0"
                            variant="videoIframe"/>
                    </par>
                    <bgimage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                    <bgvideo
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                </contentblock_copy>
            </par>
        </article>
    </jcr:content>
</jcr:root>
