<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[swinburne:content-type/page]"
        cq:template="/conf/swinburne/settings/wcm/templates/twocolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="facebook"
        sling:resourceType="swinburne/components/template/base"
        redirectTarget="https://www.facebook.com/swinburne">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/swinburne/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    jcr:mixinTypes="[cq:Taggable]"
                    sling:resourceType="swinburne/components/details/page-details"
                    componentIcon="[swinburne:component-style-icon/social/facebook]"
                    pageIcon="[swinburne:component-style-modifier/icon/size/sm,swinburne:component-style-icon/social/facebook]"
                    componentModifiers="[swinburne:component-style-modifier/icon/size/sm]"
                    icon="fa fa-facebook"
                    title="Join us on Facebook">
                </page-details>
            </par>
        </article>
    </jcr:content>
</jcr:root>
