<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
          cq:template="/conf/swinburne/settings/wcm/templates/onecolumn"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Apply Now"
         sling:resourceType="swinburne/components/template/base">

        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article"
            componentId="article_apply_now">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page_details
                      jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/details/page-details"
                    componentId="page_details_apply_now"
                    hideDescription="false"
                    hideTitle="false"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="false"
                    showToolbar="false"
                    textIsRich="true"
                    titleIcon="[swinburne:component-style-icon/general/edit]"
                    titleIconShow="true">
                    <secondaryImage
                          jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                    <thumbnail
                          jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                    <bgimage
                          jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                </page_details>
            </par>
        </article>

    </jcr:content>
</jcr:root>
