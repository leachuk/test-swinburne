<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
          cq:template="/conf/swinburne/settings/wcm/templates/onecolumn"
        jcr:primaryType="cq:PageContent"
        jcr:title="Swinburne Info Evening"
        sling:resourceType="swinburne/components/template/base">

        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article"
            componentId="article_jie">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page_details
                      jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/details/page-details"
                    componentId="page_details_jie"
                    componentTheme="[swinburne:component-style-theme/layout/page-details/theme--pagedetail-brand]"
                    hideDescription="true"
                    hideTitle="true"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showParsys="true"
                    showToolbar="false"
                    textIsRich="true">
                    <secondaryImage
                          jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                    <thumbnail
                          jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                    <bgimage
                          jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"
                        fileReference="/content/dam/swinburne-showcase/en/common/move-into-midyear.jpg"/>
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <image
                              jcr:primaryType="nt:unstructured"
                            sling:resourceType="aemdesign/components/media/image"
                            componentId="image_jie"
                            fileReference="/content/dam/swinburne-showcase/en/common/jump-in-event.png"/>
                        <text
                              jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/text"
                            componentId="text_jie"
                            text="&lt;p>It’s time to stop thinking, start doing, and step into study. Find out all you need to know about our course options and how you can start studying in 2018.&lt;/p>&#xd;&#xa;"
                            textIsRich="true"/>
                        <pagetitle
                              jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/pagetitle"
                            componentId="pagetitle_jie"/>
                        <text_copy
                              jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/text"
                            componentId="text_jie"
                            text="&lt;p>Tuesday 23 January, &lt;g class=&quot;gr_ gr_9 gr-alert gr_gramm gr_inline_cards gr_run_anim Style multiReplace&quot; id=&quot;9&quot; data-gr-id=&quot;9&quot;>4pm&lt;/g> – &lt;g class=&quot;gr_ gr_10 gr-alert gr_gramm gr_inline_cards gr_disable_anim_appear Style multiReplace&quot; id=&quot;10&quot; data-gr-id=&quot;10&quot;>6pm&lt;/g>&lt;/p>&#xd;&#xa;&lt;p>Hawthorn campus&lt;/p>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link
                              jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/link"
                            componentId="link_jie"
                            componentModifiers="[swinburne:component-style-modifier/button/normal/secondary]"
                            label="Register Now"
                            linkUrl="/content/swinburne/en"/>
                    </par>
                </page_details>
                <contentblock
                      jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblock"
                    componentId="contentblock_jie"
                    componentTheme="[swinburne:component-style-theme/layout/contentblock/banner]"
                    variant="container">
                    <bgimage
                          jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"
                        fileReference="/content/dam/swinburne/juggling.jpg"/>
                    <bgvideo
                          jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/media/image"/>
                    <aemform
                          jcr:primaryType="nt:unstructured"
                        sling:resourceType="fd/af/components/aemform"
                        formRef="/content/dam/formsanddocuments/we-finance/test"
                        formType="adaptiveForm"
                        submitType="inline"
                        textIsRich="true"
                        thankyouConfig="message"
                        useiframe="true"/>
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="aemdesign/components/layout/container">
                        <text
                              jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/text"
                            componentId="text_jie"
                            componentModifiers="[swinburne:component-style-modifier/text/transform/uppercase,swinburne:component-style-modifier/font/font-weight-bolder]"
                            text="&lt;h2>JUGGLING LIFE AND UNI?&lt;/h2>&#xd;&#xa;"
                            textIsRich="true"/>
                        <link
                              jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/link"
                            componentId="link_jie"
                            componentModifiers="[swinburne:component-style-modifier/button/normal/secondary]"
                            label="Tell Me How"
                            variant="button"/>
                    </par>
                </contentblock>
            </par>
        </article>

    </jcr:content>
</jcr:root>
