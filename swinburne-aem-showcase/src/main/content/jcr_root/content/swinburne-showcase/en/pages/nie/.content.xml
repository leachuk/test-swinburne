                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="swinburne/components/layout/contentblock"
                                        componentId="contentblock11d"
                                        dataParent="contentblock11"
                                        dataToggle="collapse"
                                        hideTitle="false"
                                        hideTitleSeparator="true"
                                        title="I'm so used to high school. What if I miss my mates at uni?"
                                        variant="childsection">
