<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/swinburne/settings/wcm/templates/onecolumn-v2"
        jcr:primaryType="cq:PageContent"
        jcr:title="Article list"
        sling:resourceType="swinburne/components/template/column/one/v2">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/header"
            componentId="header">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
        </header>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article"
            componentId="article_">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
        </article>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/footer"
            componentId="footer">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
        </footer>
    </jcr:content>
    <article-1/>
    <article-2/>
    <article-3/>
    <article-4/>
</jcr:root>
