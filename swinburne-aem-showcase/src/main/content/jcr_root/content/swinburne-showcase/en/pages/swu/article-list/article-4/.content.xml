<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/swinburne/settings/wcm/templates/onecolumn-v2"
        jcr:primaryType="cq:PageContent"
        jcr:title="Article 4"
        sling:resourceType="swinburne/components/template/column/one/v2">
        <header
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/header"
            componentId="header">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
        </header>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article"
            componentId="article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <news_details
                    cq:tags="[swinburne-showcase:component-style-icon/social/instagram,swinburne-showcase:component-style-icon/social/youtube]"
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/details/news-details"
                    componentId="news_details"
                    textIsRich="true"
                    titleType="[,]">
                    <secondaryImage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/componentsmedia/image"/>
                    <thumbnail
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/componentsmedia/image"
                        fileReference="/content/dam/swinburne-showcase/en/components/media/image/city1.jpg"/>
                    <bgimage
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/componentsmedia/image"/>
                </news_details>
            </par>
        </article>
        <footer
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/footer"
            componentId="footer">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
        </footer>
    </jcr:content>
</jcr:root>
