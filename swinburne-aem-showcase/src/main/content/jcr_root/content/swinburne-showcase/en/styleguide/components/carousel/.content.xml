<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:template="/conf/swinburne/settings/wcm/templates/styleguide"
        jcr:primaryType="cq:PageContent"
        jcr:title="Carousel"
        sling:resourceType="swinburne/components/template/styleguide">
        <aside
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/aside"
            componentId="aside">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container"/>
        </aside>
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="swinburne/components/layout/article"
            componentId="article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page_details
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/details/page-details"
                    componentId="page_details"
                    componentModifiers="[swinburne-showcase:component-style-modifier/background/primary/bg-yellow]"
                    componentTheme="[swinburne-showcase:component-style-theme/styleguide/page-details]"
                    showBreadcrumb="false"
                    showPageDate="false"
                    showToolbar="false"
                    textIsRich="true"
                    titleType="[,]"/>
                <contentblock_carousel_pagelist_default
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="contentblock_carousel_pagelist_default">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <contentblock_carousel_pagelist_default_title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                componentId="contentblock_carousel_pagelist_default_title"
                                text="&lt;h4>Basic Carousel - Default Background (Page List)&lt;/h4>&#xd;&#xa;"
                                textIsRich="true"/>
                        <contentblock_carousel_pagelist_default_list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/lists/pagelist"
                                componentId="contentblock_carousel_pagelist_default_list"
                                componentModules="[swinburne-showcase:component-style-module/lists/carousel]"
                                detailsBadge="cardImageTagTitleAction"
                                listFrom="children"
                                parentPage="/content/swinburne-showcase/en/component/lists/page-list/cards"
                                sortOrder="asc"
                                tagsMatch="any"/>
                    </par>
                </contentblock_carousel_pagelist_default>
                <contentblock_carousel_pagelist_grey
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="contentblock_carousel_pagelist_grey"
                        componentModifiers="[swinburne-showcase:component-style-modifier/background/greys/bg-grey-5]">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <contentblock_carousel_pagelist_grey_title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                componentId="contentblock_carousel_pagelist_grey_title"
                                text="&lt;h4>Basic Carousel - Grey Background (Page List)&lt;/h4>&#xd;&#xa;"
                                textIsRich="true"/>
                        <contentblock_carousel_pagelist_grey_list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/lists/pagelist"
                                componentId="contentblock_carousel_pagelist_grey_list"
                                componentModules="[swinburne-showcase:component-style-module/lists/carousel]"
                                detailsBadge="cardImageTagTitleAction"
                                listFrom="children"
                                parentPage="/content/swinburne-showcase/en/component/lists/page-list/cards"
                                sortOrder="asc"
                                tagsMatch="any"/>
                    </par>
                </contentblock_carousel_pagelist_grey>
                <contentblock_carousel_pagelist_yellow
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="contentblock_carousel_pagelist_yellow"
                        componentModifiers="[swinburne-showcase:component-style-modifier/background/primary/bg-yellow]">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <contentblock_carousel_pagelist_yellow_title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                componentId="contentblock_carousel_pagelist_yellow_title"
                                text="&lt;h4>Basic Carousel - Yellow Background (Page List)&lt;/h4>&#xd;&#xa;"
                                textIsRich="true"/>
                        <contentblock_carousel_pagelist_yellow_list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/lists/pagelist"
                                componentId="contentblock_carousel_pagelist_yellow_list"
                                componentModules="[swinburne-showcase:component-style-module/lists/carousel]"
                                detailsBadge="cardImageTagTitleAction"
                                listFrom="children"
                                parentPage="/content/swinburne-showcase/en/component/lists/page-list/cards"
                                sortOrder="asc"
                                tagsMatch="any"/>
                    </par>
                </contentblock_carousel_pagelist_yellow>
                <contentblock_carousel_pagelist_default_short
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="contentblock_carousel_pagelist_default_short">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <contentblock_carousel_pagelist_default_short_title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                componentId="contentblock_carousel_pagelist_default_short_title"
                                text="&lt;h4>Dynamic Carousel - Default Background (Page List)&lt;/h4>&lt;p>The carousel won't take affect until the screen real estate becomes too small for each item to fit.&lt;/p>&#xd;&#xa;"
                                textIsRich="true"/>
                        <contentblock_carousel_pagelist_default_short_list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/lists/pagelist"
                                componentId="contentblock_carousel_pagelist_default_short_list"
                                componentModules="[swinburne-showcase:component-style-module/lists/carousel]"
                                detailsBadge="cardImageTagTitleAction"
                                listFrom="static"
                                pages="[/content/swinburne-showcase/en/component/lists/page-list/cards/page1,/content/swinburne-showcase/en/component/lists/page-list/cards/page2,/content/swinburne-showcase/en/component/lists/page-list/cards/page3]"
                                sortOrder="asc"
                                tagsMatch="any"/>
                    </par>
                </contentblock_carousel_pagelist_default_short>
                <contentblock_carousel_newslist_default
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="contentblock_carousel_newslist_default">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <contentblock_carousel_newslist_default_title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                componentId="contentblock_carousel_newslist_default_title"
                                text="&lt;h4>Basic Carousel - Default Background (News List)&lt;/h4>&#xd;&#xa;"
                                textIsRich="true"/>
                        <contentblock_carousel_newslist_default_list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/lists/newslist"
                                componentId="contentblock_carousel_newslist_default_list"
                                componentModules="[swinburne-showcase:component-style-module/lists/carousel]"
                                detailsBadge="cardImageTitleCategoryActionDate"
                                listFrom="static"
                                pages="[/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details]"
                                sortOrder="asc"
                                tagsMatch="any"/>
                    </par>
                </contentblock_carousel_newslist_default>
                <contentblock_carousel_newslist_grey
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="contentblock_carousel_newslist_grey"
                        componentModifiers="[swinburne-showcase:component-style-modifier/background/greys/bg-grey-5]">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <contentblock_carousel_newslist_grey_title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                componentId="contentblock_carousel_newslist_grey_title"
                                text="&lt;h4>Basic Carousel - Grey Background (News List)&lt;/h4>&#xd;&#xa;"
                                textIsRich="true"/>
                        <contentblock_carousel_newslist_grey_list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/lists/newslist"
                                componentId="contentblock_carousel_newslist_grey_list"
                                componentModules="[swinburne-showcase:component-style-module/lists/carousel]"
                                detailsBadge="cardImageTitleCategoryActionDate"
                                listFrom="static"
                                pages="[/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details]"
                                sortOrder="asc"
                                tagsMatch="any"/>
                    </par>
                </contentblock_carousel_newslist_grey>
                <contentblock_carousel_newslist_yellow
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="contentblock_carousel_newslist_yellow"
                        componentModifiers="[swinburne-showcase:component-style-modifier/background/primary/bg-yellow]">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container">
                        <contentblock_carousel_newslist_yellow_title
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/text"
                                componentId="contentblock_carousel_newslist_yellow_title"
                                text="&lt;h4>Basic Carousel - Yellow Background (News List)&lt;/h4>&#xd;&#xa;"
                                textIsRich="true"/>
                        <contentblock_carousel_newslist_yellow_list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/lists/newslist"
                                componentId="contentblock_carousel_newslist_yellow_list"
                                componentModules="[swinburne-showcase:component-style-module/lists/carousel]"
                                detailsBadge="cardImageTitleCategoryActionDate"
                                listFrom="static"
                                pages="[/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details,/content/swinburne-showcase/en/styleguide/components/news-details]"
                                sortOrder="asc"
                                tagsMatch="any"/>
                    </par>
                </contentblock_carousel_newslist_yellow>
            </par>
        </article>
    </jcr:content>
</jcr:root>
