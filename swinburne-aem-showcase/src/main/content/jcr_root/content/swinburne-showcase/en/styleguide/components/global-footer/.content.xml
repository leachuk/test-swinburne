<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:template="/conf/swinburne/settings/wcm/templates/styleguide"
            jcr:primaryType="cq:PageContent"
            jcr:title="Global Footer"
            sling:resourceType="swinburne/components/template/styleguide">
        <aside
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/aside"
                componentId="aside_1PXHXBJE3">
            <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/container"/>
        </aside>
        <article
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/article"
                componentId="article_TWQKWLZ6M">
            <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/container">
                <page_details
                        jcr:mixinTypes="[cq:Taggable]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/details/page-details"
                        componentId="page_details"
                        componentModifiers="[swinburne-showcase:component-style-modifier/background/primary/bg-yellow]"
                        componentTheme="[swinburne-showcase:component-style-theme/styleguide/page-details]"
                        editable="{Boolean}false"
                        showBreadcrumb="false"
                        showPageDate="false"
                        showParsys="false"
                        showToolbar="false"
                        textIsRich="true">
                </page_details>
                <contentblock
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/contentblock"
                        componentId="contentblock">
                    <par
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/layout/container"
                            editable="{Boolean}true">
                        <reference
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="swinburne/components/content/reference"
                                componentId="reference"
                                path="/content/swinburne-showcase/en/component/layout/footer/jcr:content/article/par/swinburne_global_footer/par/footer" />
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
