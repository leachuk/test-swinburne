<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            cq:template="/conf/swinburne/settings/wcm/templates/styleguide"
            jcr:primaryType="cq:PageContent"
            jcr:title="News Details"
            sling:resourceType="swinburne/components/template/styleguide">
        <aside
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/aside"
                componentId="aside_OWMQFYPNU">
            <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/container"/>
        </aside>
        <article
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/article"
                componentId="article_KKONTFIN2">
            <par
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/container">
                <news_details
                        cq:tags="[swinburne-showcase:content-type/widgets/onlinemedia/provider/youtube,swinburne-showcase:content-type/asset/audio,swinburne-showcase:content-type/asset/document]"
                        jcr:mixinTypes="[cq:Taggable]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/details/news-details"
                        badgeLinkStyle="[swinburne-showcase:component-style-modifier/button/primary]"
                        badgeLinkText="CTA link"
                        badgeLinkTitle="CTA link"
                        componentId="news_details"
                        componentTheme="[swinburne-showcase:component-style-theme/layout/news-details/theme--news-details]"
                        textIsRich="true"
                        titleType="[,]">
                    <thumbnail
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/media/image"
                            fileReference="/content/dam/swinburne-showcase/en/components/content/download/change-as-good-holiday.jpg"/>
                </news_details>
            </par>
        </article>
    </jcr:content>
</jcr:root>
