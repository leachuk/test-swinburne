<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:tags="[swinburne:content-type/page]"
        cq:template="/apps/swinburne/templates/column/two"
        jcr:isCheckedOut="{Boolean}true"
        jcr:mixinTypes="[mix:lockable,mix:versionable]"
        jcr:primaryType="cq:PageContent"
        jcr:title="Replicating Packages"
        sling:resourceType="swinburne/components/template/column/two">
        <article
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/swinburne/components/layout/article">
            <par
                jcr:primaryType="nt:unstructured"
                sling:resourceType="swinburne/components/layout/container">
                <page-details
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/details/page-details"/>
                <pagelist
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/lists/pagelist"
                    componentTheme="[]"
                    listFrom="children"
                    listType="list"
                    parentPage="/content/swinburne-training/en/aem-training/administrator/packaging"
                    printStructure="{Boolean}true"
                    tagsMatch="any"/>
                <contentblock
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="swinburne/components/layout/contentblock"
                    componentTheme="[]"
                    hideTitle="true"
                    hideTitleSeparator="true">
                    <par
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="swinburne/components/layout/container">
                        <text_1907134826
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/text"
                            componentTheme="[]"
                            text="&lt;p>Replicate the contents of a package to install it to the publish instance:&lt;/p>&#xa;&lt;ul>&#xa;&lt;li>In the &lt;b>Package Manager&lt;/b>, navigate to the package that you want to replicate.&lt;/li>&#xa;&lt;li>Click the icon or the name of the package you want to replicate to expand it.&lt;/li>&#xa;&lt;li>In the &lt;b>More&lt;/b> drop-down menu on the toolbar, select &lt;b>Replicate&lt;/b>.&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                        <image_1586087139
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/media/image"
                            fileReference="/content/dam/swinburne-training/Packaging-Replicate_Package.png"
                            imageOption="fixedImageGenerated"
                            imageRotate="0"
                            width="500"/>
                        <text
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="swinburne/components/content/text"
                            componentTheme="[]"
                            text="&lt;ul>&#xa;&lt;li>Confirm the action by clicking &lt;b>Yes&lt;/b> on the confirmation box.&lt;/li>&#xa;&lt;/ul>&#xa;"
                            textIsRich="true"/>
                    </par>
                </contentblock>
            </par>
        </article>
    </jcr:content>
</jcr:root>
